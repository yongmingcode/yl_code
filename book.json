{
    "title": "Java 源码分析",
    "description": "java知识点分析记录",
    "author": "yongming",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "direction" : "ltr",
    "root": ".",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "links": {
        "sidebar": {
            "我的主页": "http://www.mayongming.cn"
        }
    },
    "plugins": [
        "github",
        "pageview-count",
        "mermaid-gb3",
        "-lunr", 
        "-search", 
        "search-plus",
        "splitter",
        "-sharing", 
        "sharing-plus",
        "expandable-chapters-small",
        "anchor-navigation-ex",
        "edit-link",
        "copy-code-button",
        "chart",
        "favicon-plus",
        "donate"
    ],
    "pluginsConfig": {
        "github": {
          "url": "https://github.com/yongmingcode/yl_java"
        },
        "sharing": {
           "douban": true,
           "facebook": false,
           "google": false,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": true,
           "qzone": true,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "facebook", "google", "twitter",
               "weibo", "instapaper", "linkedin",
               "pocket", "stumbleupon"
           ]
       },
       "edit-link": {
          "base": "https://github.com/yongmingcode/yl_java/blob/master",
          "label": "编辑本页"
        },
        "chart": {
          "type": "c3"
        },
        "favicon": "/images/favicon.ico",
        "appleTouchIconPrecomposed152": "/images/apple-touch-icon-precomposed-152.png",
        "output": "_book",
        "donate": {
          "wechat": "images/wechat.png",
          "alipay": "images/alipay.png",
          "button": "赏",
          "alipayText": "支付宝",
          "wechatText": "微信"
        }
    },
    "styles": {
      "website": "styles/website.css"
    }
}